import java.util.Scanner;
public class Main {
    public static void main (String[] args){
        Scanner input = new Scanner(System.in);
        int n  = input.nextInt();
        input.nextLine();
        for (  int i = 0 ; i < n ; i++) {
            String Str = input.nextLine();

            //if ( ch == "YES" )
            if (Str.equalsIgnoreCase("YES"))
            {
                System.out.println("YES");
            } else {
                System.out.println("NO");
            }

        }
    }
}
