import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int x = input.nextInt();
        int y = input.nextInt();
        int count = 0;
        while (x <= y ) {
            x = x * 3;
            y = y * 2;
            count++;
        }
        System.out.println(count);
    }
}

